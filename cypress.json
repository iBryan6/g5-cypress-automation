{
    "defaultCommandTimeout": 10000,
    "env": {
        "TESTING_ENV": "staging",
        "BASE_URL_STAGING": "https://content-management-system-content-staging.g5devops.com",
        "BASE_URL_PRIME": "https://content-management-system-content-staging.g5devops.com",
        "BASE_URL_PROD": "https://cms.g5marketingcloud.com"
    },
    "reporter": "mochawesome",
    "reporterOptions": {
        "reportDir": "cypress/report/mochawesome-report",
        "overwrite": false,
        "html": true,
        "json": false,
        "timestamp": "mmddyyyy_HHMMss"
    },
    "videosFolder": "cypress/report/videos",
    "screenshotsFolder": "cypress/report/screenshots"
}